{
    "name": "darkwebdesign/doctrine-enhanced-events",
    "type": "library",
    "description": "Doctrine Enhanced Events",
    "keywords": ["doctrine", "events"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "doctrine/common": "^2.2",
        "doctrine/orm": "^2.2",
        "php": ">=5.3.3"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "darkwebdesign/doctrine-unit-testing": "^2.4",
        "phpunit/phpunit": "^4.8"
    },
    "suggest": {
        "darkwebdesign/doctrine-enhancement-pack": "All Doctrine enhancements bundled together"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhanced\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhanced\\Event\\Tests\\": "tests/"
        }
    }
}
