{
    "name": "darkwebdesign/doctrine-enhancement-pack",
    "type": "library",
    "description": "Doctrine Enhancement Pack",
    "keywords": ["doctrine", "events"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "doctrine/event-manager": "^1.1",
        "doctrine/orm": "2.7.*",
        "doctrine/persistence": "^1.3.3 || ^2.0",
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "darkwebdesign/doctrine-unit-testing": "2.7.*",
        "doctrine/data-fixtures": "^1.1",
        "phpunit/phpunit": "^7.5"
    },
    "replace": {
        "darkwebdesign/doctrine-enhanced-events": "self.version"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhancedEvents\\": "doctrine-enhanced-events/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhancedEvents\\Tests\\": "doctrine-enhanced-events/tests/"
        }
    }
}
