{
    "name": "darkwebdesign/doctrine-enhancement-pack",
    "type": "library",
    "description": "Doctrine Enhancement Pack",
    "keywords": ["doctrine", "events", "repositories"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "doctrine/common": "^2.2",
        "doctrine/orm": "^2.2",
        "php": ">=5.3.3"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "darkwebdesign/doctrine-unit-testing": "^2.4",
        "phpunit/phpunit": "^4.8"
    },
    "replace": {
        "darkwebdesign/doctrine-enhanced-events": "self.version",
        "darkwebdesign/doctrine-enhanced-repositories": "self.version"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhanced\\Event\\": "doctrine-enhanced-events/src/",
            "DarkWebDesign\\DoctrineEnhanced\\Repository\\": "doctrine-enhanced-repositories/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DarkWebDesign\\DoctrineEnhanced\\Event\\Tests\\": "doctrine-enhanced-events/tests/",
            "DarkWebDesign\\DoctrineEnhanced\\Repository\\Tests\\": "doctrine-enhanced-repositories/tests/"
        }
    }
}
